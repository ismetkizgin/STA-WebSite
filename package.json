{
  "name": "sta-website",
  "version": "1.5.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build & ng serve --configuration=production --host 0.0.0.0",
    "start:stage": "ng serve --configuration=stage --host 0.0.0.0",
    "start:dev": "ng serve",
    "start:heroku": "node serve.js -p $PORT",
    "heroku-postbuild": "ng build --configuration=stage",
    "build": "ng build --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _",
    "release": "standard-version"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.11",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "~10.0.11",
